{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node",
            "config": {
                "runtime": "nodejs/16" // Specify desired Node.js runtime version
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api/*" // Forward all requests to your Express.js API routes
        }
    ]
}